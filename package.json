{
  "name": "UniversitiesSearch",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "tsc": "tsc",
    "lint:staged": "lint-staged",
    "lint": "yarn typecheck && yarn run lint:eslint:fix",
    "lint:js": "yarn lint:eslint . ",
    "lint:eslint:quiet": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --quiet",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --ignore-path .gitignore --ignore-pattern internals/scripts --ignore-pattern server",
    "lint:eslint:fix": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --ignore-path .gitignore --ignore-pattern internals/scripts --ignore-pattern server --fix"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "git add --force"
    ],
    "*.ts": [
      "yarn lint",
      "git add --force"
    ],
    "*.tsx": [
      "yarn lint",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
